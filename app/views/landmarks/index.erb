<h1>Landmarks Index</h1>
<table>
  <tr>
    <th>Landmark Name</th>
    <th>Year Completed</th>
  </tr>
  <%@landmarks.each do |landmark|%>
    <tr>
      <td><a href="/landmarks/<%=landmark.id%>"><%= landmark.name %></td>
      <td><%= landmark.year_completed %></td>
    </tr>
  <%end%>
